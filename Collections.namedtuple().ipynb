    "    arr.append(list(map(str,input().split())))\n",
    "\n",
    "\n",
    "pos = arr[0].index('MARKS')\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    answer += float(arr[i][pos])\n",
    "\n",
    "print(answer/n)"
   ]
